System.out.println("Insert Form feed in the text at this point \f in java");
System.out.println("Insert Tab in text at this point \t in java");
System.out.println("Insert Backslash character in text at this point \\ in java");
System.out.println("Insert Single quote in text at this point \' in java");
System.out.println("Insert Double quote character in text at this point \" in java");
}
